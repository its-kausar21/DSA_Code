class Solution {
    // Function to insert a node at the end of the linked list.
    Node insertAtEnd(Node head, int x) {
        // code here
        
        Node n=new Node(x);
        
        if(head==null){
            return head;
        }
        
        Node temp=head;
        
        while(temp.next!=null){
            temp=temp.next;
        }
        
        temp.next=n;
        
        return head;
    }
}
